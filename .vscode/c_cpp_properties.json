{
    "configurations": [
        {
            "name": "Win64",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/dependencies/**/include",
                "${config:DEFAULT_PATH}/lib"
            ],
            "compilerPath": "${config:DEFAULT_PATH}\\${config:G++_EXE}",
            "cStandard": "c23",
            "cppStandard": "c++17",
            "intelliSenseMode": "${default}",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}